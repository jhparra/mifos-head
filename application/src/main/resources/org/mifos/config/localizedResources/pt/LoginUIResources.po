msgstr "Nova Password"

#: login.oldpassword
msgid "Old Password"
msgstr "Password Antiga"

#: login.password
msgid "Password"
msgstr "Password"

#: login.submit
msgid "Submit"
msgstr "Submiter"

#: login.username
